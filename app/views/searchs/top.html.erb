<% if params[:search].present? %>
    <% if params[:model] == "user" %>
        <% @users.each do |f| %>
             <%= image_tag f.get_profile_image, size: '100x100' %>
             <%= f.name %>
         <% end %>
    <% elsif params[:model] == "book"%>
         <% @books.each do |f| %>
             <%= f.title %>
             <%= f.body %>
         <% end %>
    <% end %>
<% else %>
 検索結果は存在しません
<% end %>
